<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nwhacks.tree_tapper" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name="nwhacks.tree_tapper.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nwhacks.tree_tapper.GameActivity"
            android:label="@string/title_activity_game"
            android:parentActivityName="nwhacks.tree_tapper.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="nwhacks.tree_tapper.MainActivity" />
        </activity>
        <activity
            android:name=".Start"
            android:label="@string/title_activity_start" >
        </activity>
        <activity
            android:name=".HighScore"
            android:label="@string/title_activity_high_score" >
        </activity>
    </application>

</manifest>
